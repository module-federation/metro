{
  "name": "module-federation-metro",
  "version": "0.0.1",
  "description": "Module Federation for Metro bundler",
  "author": "",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "dev": "./dev/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./commands": {
      "types": "./dist/commands/index.d.ts",
      "dev": "./dev/commands.js",
      "import": "./dist/commands/index.mjs",
      "default": "./dist/commands/index.js"
    },
    "./babel/*": "./babel/*.js",
    "./async-startup": {
      "dev": "./src/modules/asyncStartup.ts",
      "default": "./dist/modules/asyncStartup.ts"
    },
    "./runtime-plugin": {
      "dev": "./src/modules/metroCorePlugin.ts",
      "default": "./dist/modules/metroCorePlugin.ts"
    }
  },
  "files": ["dist", "babel", "bootstrap"],
  "scripts": {
    "build": "rslib build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "@module-federation/runtime": "0.11.4",
    "@module-federation/sdk": "0.11.4"
  },
  "peerDependencies": {
    "metro": "^0.82.1",
    "metro-config": "^0.82.1",
    "metro-resolver": "^0.82.1",
    "metro-source-map": "^0.82.1",
    "react-native": ">=0.79.0"
  },
  "devDependencies": {
    "@rslib/core": "^0.10.0",
    "@types/react": "^19.0.0",
    "@types/node": "^20.0.0",
    "metro": "^0.82.1",
    "metro-config": "^0.82.1",
    "metro-resolver": "^0.82.1",
    "metro-source-map": "^0.82.1",
    "react-native": "0.79.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
