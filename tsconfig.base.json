{
  "compilerOptions": {
    "module": "preserve",
    "moduleResolution": "bundler",
    "target": "ES2021",
    "esModuleInterop": true,
    "declaration": true,
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "composite": false,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-native",
    "allowJs": false,
    "checkJs": false,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "metro/src/DeltaBundler/Serializers/baseJSBundle": [
        "./packages/core/src/types/metro/baseJSBundle"
      ],
      "metro/src/lib/bundleToString": [
        "./packages/core/src/types/metro/bundleToString"
      ],
      "metro/src/lib/getAppendScripts": [
        "./packages/core/src/types/metro/getAppendScripts"
      ],
      "metro/src/DeltaBundler/Serializers/helpers/processModules": [
        "./packages/core/src/types/metro/processModules"
      ],
      "metro/src/lib/relativizeSourceMap": [
        "./packages/core/src/types/metro/relativizeSourceMap"
      ]
    }
  }
}
